{
  "extends": ["eslint:recommended", "next"],
  "env": { "es6": true, "jest": true },
  "rules": {
    "no-case-declarations": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ]
  }
}
